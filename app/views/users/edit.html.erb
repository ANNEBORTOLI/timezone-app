<div class="global" style="background-image: linear-gradient(rgba(0,0,0,0.2),rgba(0,0,0,0.2)), url(https://images.unsplash.com/photo-1636955816868-fcb881e57954?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=1770&q=80);">
  <div class="edit-cointainer">
    <h1 class="titleedit mx-1 my-1 p-5"> <%= image_tag "userprofile.png" %> Edit profile </h1>
      <div id="formedit">
        <div class="container row justify-content-center ">
            <%= simple_form_for @user do |f| %>
              <%= f.input :address %>
              <%= f.input :working_hour_start %>
              <%= f.input :working_hour_end %>
                <%= f.button :submit, class: "btn btn-primary my-3 d-flex" %>
            <% end %>
        </div>
      </div>
  </div>

  <div class="edit-cointainer2" style="background-image: linear-gradient(rgba(0,0,0,0.2),rgba(0,0,0,0.2)), url(https://images.unsplash.com/photo-1599930113854-d6d7fd521f10?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHx8fA%3D%3D&auto=format&fit=crop&w=1180&q=80);">

  <div>
</div>
